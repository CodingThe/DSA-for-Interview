#include<bits/stdc++.h>
using namespace std;
struct node{
    int data;
    node* left;
    node* right;
    node(int x){
        data=x;
        left=NULL;
        right=NULL;
   }
};

void printlev(node* head){
  if(head==NULL){
      return;
  }
  queue<node*>q;
  q.push(head);
  q.push(NULL);
  while(q.size()>1){
      node* curr=q.front();
      q.pop();
      if(curr==NULL){
          cout<<endl;
          q.push(NULL);
          continue;
      }
      cout<<curr->data<<" ";
      if(curr->left!=NULL){
          q.push(curr->left);
      }
      if(curr->right!=NULL){
          q.push(curr->right);
      }
  }
}
int main(){
     node* head=new node(100);
     head->left=new node(200);
     head->right=new node(300);
     head->left->left=new node(400);
     head->left->right=new node(500);
     head->right->right=new node(700);
    printlev(head);
    
    return 0;
    
}
