#include<bits/stdc++.h>
using namespace std;
int main(){
 vector<int>v;
 
 // Insertion in vector
 v.push_back(100);
 v.push_back(200);
 v.push_back(300);
 v.push_back(400);
 v.push_back(500);
 
 //size of vector
 int x=v.size();
 cout<<"The size of vector is: "<<x<<endl;
 
 // Traversal of vector
 for(int i=0;i<v.size();i++){
     cout<<v[i]<<" ";
 }
 cout<<endl;
 
 // Deletion of values in vector
 v.pop_back();
 v.pop_back();
 
 // Traversal of vector
  for(auto &it:v){
     cout<<it<<" ";
 }
 cout<<endl;
 
 // Searching of an element in vector
 int q=300;
 for(int i=0;i<v.size();i++){
     if(v[i]==q){
         cout<<"Value 300 found at : "<<i<<" location"<<endl;
     }
 }
 
 
 //Finding last element in vector
 cout<<"The last element of the vector is: "<<v.back()<<endl;
 
 
 // sorting of vector
 cout<<"Vector after sorting: "<<endl;
 sort(v.begin(),v.end());
 for(auto &it:v){
     cout<<it<<" ";
 }
 cout<<endl;
 
 //reverse a Vector
 reverse(v.begin(),v.end());
 cout<<"Vector after reversal : "<<endl;
 for(auto &it:v){
     cout<<it<<" ";
 }
 cout<<endl;
return 0; 
}
